{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"react/react-in-jsx-scope": "off"
	},
	"globals": {
		"React": "writable"
	},
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"ecmaFeatures": {
		"jsx": true,
		"modules": true
	}
}
